<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".feature.profile.view.ProfileOptionFragment"
    android:orientation="vertical">
<!--&lt;!&ndash;<LinearLayout&ndash;&gt;-->
<!--&lt;!&ndash;android:layout_width="match_parent"&ndash;&gt;-->
<!--&lt;!&ndash;android:layout_height="match_parent"&ndash;&gt;-->
<!--&lt;!&ndash;android:orientation="vertical">&ndash;&gt;-->

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/reauty_yellow_variant">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableStart="@drawable/ic_baseline_arrow_back_ios_24"
            android:textSize="20dp"
            android:textColor="#8B9EC4"
            android:id="@+id/btn_myProfileTV"
            android:padding="10dp"
            android:fontFamily="@font/ivymode_semibold"
            android:text="@string/my_profile"/>
    </androidx.appcompat.widget.LinearLayoutCompat>

    <TextView
        android:textColor="@color/text_black"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Account"
        android:padding="20dp"
        android:textAllCaps="true"/>


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_profOptions"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        android:textColor="@color/text_black"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/setttings"
        android:padding="20dp"
        android:textAllCaps="true"/>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_profOptions2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</LinearLayout>